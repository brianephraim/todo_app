{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
  },
  "rules": {
    "import/no-unresolved": 0,
    "new-cap": 0,
    "arrow-body-style": ["error", "always"],
    "react/prefer-stateless-function": 0,
    "no-unused-expressions": 0,
    "no-param-reassign": 0,
    "no-throw-literal": 0,
    "no-shadow": 0,
    "import/extensions": 0
  }
}